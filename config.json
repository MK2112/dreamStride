{
    "env": "spot-walk",
    "algo": "Dreamerv2",
    "exp": "spot-webots",
    "seed": 1,
    "max_episode_length": 1000,
    "buffer_size": 1000000,
    "time_limit": 2000,
    "cnn_activation_function": "relu",
    "dense_activation_function": "elu",
    "obs_embed_size": 1024,
    "num_units": 400,
    "deter_size": 200,
    "stoch_size": 32,
    "action_repeat": 2,
    "action_noise": 0.3,
    "total_steps": 5000000,
    "seed_steps": 5000,
    "update_steps": 100,
    "collect_steps": 1000,
    "batch_size": 50,
    "train_seq_len": 50,
    "imagine_horizon": 15,
    "free_nats": 3.0,
    "discount": 0.99,
    "td_lambda": 0.95,
    "kl_loss_coeff": 1.0,
    "kl_alpha": 0.8,
    "disc_loss_coeff": 10.0,
    "model_learning_rate": 6e-4,
    "actor_learning_rate": 8e-5,
    "value_learning_rate": 8e-5,
    "adam_epsilon": 1e-7,
    "grad_clip_norm": 100.0,
    "test_interval": 10000,
    "test_episodes": 10,
    "scalar_freq": 1000,
    "log_video_freq": -1,
    "max_videos_to_save": 2,
    "checkpoint_interval": 10000,
    "socket": {
        "host": "localhost",
        "port_controller": 9998,
        "port_daydreamer": 9999,
        "buffer_size": 1024,
        "recv_size": 8
    }
}
